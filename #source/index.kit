<html lang="ru">

<head>
	<title>ToDo App</title>
	<meta charset="utf=8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="format-detection" content="telephone=no">
	<link rel="stylesheet" href="css/style.css">
	<meta name="viewport" content="width=device-width">
	<link rel="shortcut icon" href="">
	<script src="js/home-script.js" defer></script>
	<script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
</head>

<body class="select">

	<lottie-player src="https://assets4.lottiefiles.com/packages/lf20_2BTmhJ.json" background="transparent" speed="1"
		loop autoplay></lottie-player>
	<div class="container">

		<div class="home__flex">
			<div class="home__title-links">
				<h1>
					<div class="hide-title">
						<div class="animate toggle-mobile-first">Hello.</div>
					</div>
					<div class="hide-title">
						<div class="animate delay02 toggle-mobile-first">I'm</div>
					</div>
					<div class="hide-title">
						<div class="animate delay04 toggle-mobile-first">Front-End</div>
					</div>
					<div class="hide-title">
						<div class="animate delay06 toggle-mobile-first">Developer</div>
					</div>
				</h1>
				<h1>
					<div class="home__link">
						<a href="./about-me.html">
							<div class="hide-web animate label-hello"></div>
							<div class="animate color-blue on-hover-40 hide-mobile toggle-mobile-second">About me</div>
						</a>
					</div>
					<div class="home__link">
						<a href="./works.html">
							<div class="hide-web animate label-iam"></div>
							<div class="animate color-blue on-hover-40 delay02 hide-mobile toggle-mobile-second">Resume</div>
						</a>
					</div>
					<div class="home__link">
						<a href="./resume.html">
							<div class="hide-web animate label-front-end"></div>
							<div class="animate color-blue on-hover-40 delay04 hide-mobile toggle-mobile-second">Works
							</div>
						</a>
					</div>
					<div class="home__link">
						<a href="./contacts.html">
							<div class="hide-web animate label-developer"></div>
							<div class="animate color-blue on-hover-40 delay06 hide-mobile toggle-mobile-second">Contact me
							</div>
						</a>
					</div>
				</h1>
				<div class="tap-info">Tap anywhere</div>
			</div>
			<!-- <div class="git-hub">
				<main id="main">
